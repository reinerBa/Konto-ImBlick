{
    "name": "konto-imblick",
    "version": "0.0.0",
    "author": "reinerBa <reinerbam@mailbox.org>",
    "description": "shows the transations in charts",
    "license": "MIT",
    "main": "./dist/electron/main.js",
    "scripts": {
        "build": "node .electron-vue/build.js && electron-builder",
        "build:dir": "node .electron-vue/build.js && electron-builder --dir",
        "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
        "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
        "dev": "node .electron-vue/dev-runner.js",
        "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
        "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
        "pack": "npm run pack:main && npm run pack:renderer",
        "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
        "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
        "test": "npm run unit",
        "postinstall": "npm run lint:fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/reinerBa/Konto-ImBlick.git"
    },
    "keywords": [
        "FinTs",
        "bankaccount",
        "charts",
        "accounting",
        "money"
    ],
    "build": {
        "productName": "konto-imblick",
        "appId": "org.simulatedgreg.electron-vue",
        "directories": {
            "output": "build"
        },
        "files": [
            "dist/electron/**/*"
        ],
        "dmg": {
            "contents": [
                {
                    "x": 410,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                },
                {
                    "x": 130,
                    "y": 150,
                    "type": "file"
                }
            ]
        },
        "mac": {
            "icon": "build/icons/icon.icns"
        },
        "win": {
            "icon": "build/icons/icon.ico"
        },
        "linux": {
            "icon": "build/icons"
        }
    },
    "dependencies": {
        "crypto-js": "^3.1.9-1",
        "electron-log": "^2.2.8",
        "electron-store": "^1.3.0",
        "highcharts": "^11.2.0",
        "json-stable-stringify": "^1.0.1",
        "krypt": "^0.2.2",
        "lokijs": "^1.5.3",
        "nedb": "^1.8.0",
        "object-hash": "^1.3.0",
        "open-fin-ts-js-client": "^2.0.4",
        "open-fin-ts-js-client-promise": "^3.0.3",
        "simple-encryptor": "^1.2.0",
        "vue": "^2.5.16",
        "vue-electron": "^1.0.6",
        "vue-responsive": "^0.1.9",
        "vue-router": "^2.5.3",
        "vue-schart": "^0.1.4",
        "vuejs-datepicker": "^0.9.29",
        "vuex": "^2.3.1"
    },
    "devDependencies": {
        "babel-core": "^6.26.2",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^9.1.3",
        "babel-plugin-istanbul": "^6.1.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.24.1",
        "babili-webpack-plugin": "^0.0.0",
        "cfonts": "^1.2.0",
        "chalk": "^2.4.1",
        "copy-webpack-plugin": "^11.0.0",
        "cross-env": "^5.1.4",
        "css-loader": "^6.8.1",
        "del": "^3.0.0",
        "devtron": "^1.4.0",
        "electron": "^27.1.2",
        "electron-builder": "^24.9.1",
        "electron-debug": "^1.5.0",
        "electron-devtools-installer": "^2.2.3",
        "eslint": "^8.54.0",
        "eslint-config-standard": "^10.2.1",
        "eslint-friendly-formatter": "^3.0.0",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-html": "^3.1.1",
        "eslint-plugin-import": "^2.11.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^3.7.0",
        "eslint-plugin-standard": "^3.1.0",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "^0.11.2",
        "html-webpack-plugin": "^5.5.3",
        "inject-loader": "^4.0.1",
        "multispinner": "^0.2.1",
        "node-loader": "^0.6.0",
        "style-loader": "^3.3.3",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "url-loader": "^4.1.1",
        "vue-html-loader": "^1.2.4",
        "vue-loader": "^17.3.1",
        "vue-style-loader": "^3.1.2",
        "vue-template-compiler": "^2.5.16",
        "webpack": "^5.89.0",
        "webpack-dev-server": "^4.15.1",
        "webpack-hot-middleware": "^2.22.1",
        "webpack-merge": "^4.1.2"
    }
}
